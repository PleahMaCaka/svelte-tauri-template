{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "svelte-tauri-template",
  "version": "0.1.0",
  "identifier": "io.github.pleahmacaka.svelte-tauri-template",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://0.0.0.0:1420",
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "svelte-tauri-template",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
